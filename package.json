{
  "name": "@ttionya/tsconfig",
  "version": "3.0.0",
  "description": "Shared TypeScript config for my projects",
  "keywords": [
    "tsconfig",
    "typescript",
    "ts",
    "config",
    "configuration"
  ],
  "author": "ttionya <git@ttionya.com>",
  "license": "MIT",
  "homepage": "https://github.com/ttionya/tsconfig",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttionya/tsconfig.git"
  },
  "bugs": {
    "url": "https://github.com/ttionya/tsconfig/issues"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "main": "tsconfig.json",
  "files": [
    "tsconfig.json",
    "tsconfig.classic.json",
    "tsconfig.commonjs.json",
    "tsconfig.esm.json"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "lint": "prettier tsconfig.* --write",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "devDependencies": {
    "@ttionya/prettier-config": "^2.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.0"
  }
}
